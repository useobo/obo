{
  "name": "@useobo/providers",
  "version": "0.0.1",
  "description": "OBO Providers - Target integrations (GitHub, Supabase, OBO itself)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./github": {
      "types": "./dist/github/index.d.ts",
      "import": "./dist/github/index.js"
    },
    "./supabase": {
      "types": "./dist/supabase/index.d.ts",
      "import": "./dist/supabase/index.js"
    },
    "./obo": {
      "types": "./dist/obo/index.d.ts",
      "import": "./dist/obo/index.js"
    },
    "./vercel": {
      "types": "./dist/vercel/index.d.ts",
      "import": "./dist/vercel/index.js"
    },
    "./slack": {
      "types": "./dist/slack/index.d.ts",
      "import": "./dist/slack/index.js"
    },
    "./linear": {
      "types": "./dist/linear/index.d.ts",
      "import": "./dist/linear/index.js"
    },
    "./notion": {
      "types": "./dist/notion/index.d.ts",
      "import": "./dist/notion/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/useobo/obo",
    "directory": "packages/providers"
  },
  "keywords": [
    "obo",
    "github",
    "supabase",
    "oauth",
    "providers"
  ],
  "author": "Kyle To <kyle@kyleto.com>",
  "license": "MIT",
  "dependencies": {
    "@useobo/core": "workspace:*",
    "@useobo/crypto": "workspace:*",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
